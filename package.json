{
  "name": "ai-email-project",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "preflight": "node ./scripts/preflight.mjs",
    "typecheck": "node ./scripts/typecheck.mjs",
    "smoke:correlation": "node ./scripts/smoke-correlation.mjs",
    "smoke:notify-dedupe": "node ./scripts/smoke-notification-dedupe.mjs",
    "smoke:notify-fanout": "node ./scripts/smoke-notification-fanout.mjs",
    "smoke:notify-coalesce": "node ./scripts/smoke-notification-coalesce.mjs",
    "smoke:notify-historyid": "node ./scripts/smoke-notification-historyid-precision.mjs",
    "smoke:notify-poison": "node ./scripts/smoke-notification-poison.mjs",
    "queue:replay": "node ./scripts/queue-replay-failed.mjs",
    "queue:status": "node ./scripts/queue-status.mjs",
    "queue:pause": "ACTION=pause node ./scripts/queue-control.mjs",
    "queue:resume": "ACTION=resume node ./scripts/queue-control.mjs",
    "queue:is-paused": "ACTION=is-paused node ./scripts/queue-control.mjs",
    "failures:list": "node ./scripts/failures-list.mjs",
    "failures:show": "node ./scripts/failures-show.mjs",
    "ops:triage": "node ./scripts/ops-triage.mjs",
    "ops:monitor": "node ./scripts/ops-monitor.mjs",
    "ops:alert-drill": "node ./scripts/ops-alert-drill.mjs",
    "mail:receipts:list": "node ./scripts/mail-receipts-list.mjs",
    "mail:receipts:show": "node ./scripts/mail-receipts-show.mjs",
    "mailbox:sync:list": "node ./scripts/mailbox-sync-list.mjs",
    "mailbox:sync:show": "node ./scripts/mailbox-sync-show.mjs",
    "docs:unstick": "node ./scripts/docs-unstick.mjs",
    "kill-switch:set": "node ./scripts/kill-switch-set.mjs",
    "dev:up": "node ./scripts/dev-up.mjs",
    "dev:down": "node ./scripts/dev-down.mjs",
    "typecheck:full": "turbo run typecheck --output-logs=full",
    "typecheck:errors": "turbo run typecheck --output-logs=errors-only",
    "typecheck:new": "turbo run typecheck --output-logs=new-only",
    "typecheck:hash": "turbo run typecheck --output-logs=hash-only",
    "typecheck:debug": "turbo run typecheck -vv",
    "typecheck:filter:shared": "turbo run typecheck --filter=@ai-email/shared --output-logs=full",
    "repo:check": "pnpm -w typecheck",
    "dev:infra": "docker compose up -d",
    "dev:infra:down": "docker compose down",
    "dev:infra:logs": "docker compose logs -f --tail=200",
    "dev:infra:reset": "docker compose down -v",
    "db:migrate": "pnpm --filter @ai-email/db db:migrate",
    "db:rollback": "pnpm --filter @ai-email/db db:rollback",
    "db:create": "pnpm --filter @ai-email/db db:create",
    "db:setup": "node ./scripts/dev-db.mjs"
  },
  "devDependencies": {
    "@ai-email/shared": "workspace:*",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.15.6",
    "bullmq": "^5.12.12",
    "ioredis": "5.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3"
  }
}
